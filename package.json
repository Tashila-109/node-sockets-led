		"etherport-client": "^0.1.4",
		"express": "^4.15.3",
		"johnny-five": "^1.3.1",
		"socket.io": "^2.0.2",
		"socket.io-client": "^2.0.2"
	},
	"devDependencies": {
		"babel-core": "^6.24.1",
		"babel-loader": "^7.0.0",
		"babel-preset-env": "^1.5.1",
		"concurrently": "^3.4.0",
		"css-loader": "^0.28.4",
		"extract-text-webpack-plugin": "^2.1.0",
		"html-webpack-plugin": "^2.28.0",
		"rimraf": "^2.6.1",

{
	"name": "johnny-five-led-sockets",
	"version": "1.0.0",
	"description": "A simple project made with sockets and johnny-five",
	"main": "src/index.js",
	"scripts": {
		"clean": "rimraf dist",
		"dev": "SET NODE_ENV=development&&node server.js",
		"build": "SET NODE_ENV=production&&npm run clean && webpack -p",
		"node:dev": "SET NODE_ENV=development&&concurrently --kill-others \"npm run dev\" \"node node-mcu.js\"",
		"node": "SET NODE_ENV=production&&node node-mcu.js"
	},
